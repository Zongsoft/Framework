<?xml version="1.0" encoding="utf-8" ?>

<options>
	<option path="/Externals/Wechat">
		<platform>
			<apps default="AppId">
				<app app.name="AppId" secret="AppSecret" nonce="Token" key="SymmetricKey" />
			</apps>
		</platform>

		<applet>
			<apps default="AppId">
				<app app.name="AppId" secret="AppSecret" />
			</apps>
		</applet>

		<channel>
			<apps>
				<app app.name="AppId" secret="AppSecret" />
			</apps>
		</channel>
	</option>

	<option path="/Externals/Wechat/Paying">
		<authority authority.name="automao.saas" code="12345678" directory="./certificates/" secret="xxxxxx" fallbackUrl="http://api.automao.cn/externals/wechat/fallback/payment">
			<apps default="AppId">
				<app app.name="AppId" secret="AppSecret" />
			</apps>
		</authority>
	</option>
</options>
